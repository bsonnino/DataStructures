<UserControl x:Class="DataStructures.View.QueueElement"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="54" Width="54" Name="elQueue">
    <Grid>
        <Image Source="\Images\Cart.png" Opacity="0.6"/>
        <ContentPresenter TextElement.FontSize="14" TextElement.FontWeight="Bold"
              HorizontalAlignment="Center" VerticalAlignment="Center"
              Content="{Binding ElementName=elQueue, Path=Data, Mode=TwoWay}"/>

    </Grid>
</UserControl>
